{
    "manifest_version": 3,
    "name": "Page Logger",
    "version": "1.0",

    "permissions": ["storage"],

    "background": {
        "service_worker": "background.js"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}